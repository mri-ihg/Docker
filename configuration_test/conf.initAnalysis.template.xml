<config><settings>

<!-- NOTE: if a combination of organism/ltlibtype/pipelineversion doesn't exit but is chosen, the pipeline will NOT be started for the sample -->

	<human>
		<exomic>
			<vcf>hg19_test</vcf>
			<gatk>hg19_plus</gatk>
		</exomic>
	</human>
</settings><targets>
		<SureSelect38Mb>/usr/local/packages/seq/tools/solexa/exome38Mb/hg19/agilentExomehg19.txt</SureSelect38Mb>
		<SureSelect50Mb>/usr/local/packages/seq/tools/solexa/exome50Mb/modified.targets.txt</SureSelect50Mb>
		<SureSelect60Mbv6>/usr/local/packages/seq/tools/solexa/exome60MbV6/modified.targets.txt</SureSelect60Mbv6>
		<SureSelect50Mbv5>/usr/local/packages/seq/tools/solexa/exome50MbV5/modified.targets.txt</SureSelect50Mbv5>
		<SureSelect50Mbv4>/usr/local/packages/seq/tools/solexa/exome50MbV4/modified.targets.txt</SureSelect50Mbv4>
		<genomic>/usr/local/packages/seq/tools/solexa/exome50MbV5/modified.targets.txt</genomic>
		<mtDNA>/usr/local/packages/seq/tools/solexa/mtDNA/mtDNA.txt</mtDNA>
		<Nextera62Mb>/usr/local/packages/seq/tools/solexa/Nextera62Mb/modified.targets.txt</Nextera62Mb>
		<TruSightOneV1>/usr/local/packages/seq/tools/solexa/TruSightOneV1.1/modified.targets.txt</TruSightOneV1>
		<SeqCapEZExomeV2>/usr/local/packages/seq/tools/solexa/SeqCapEZExomeV2/modified.targets.txt</SeqCapEZExomeV2>
		<SeqCapEZExomeV3>/usr/local/packages/seq/tools/solexa/SeqCapEZExomeV3/modified.targets.txt</SeqCapEZExomeV3>
		<NexteraRapidCaptureV1.2>/usr/local/packages/seq/tools/solexa/NexteraRapidCaptureV1.2/modified.targets.txt</NexteraRapidCaptureV1.2>
		<TruSeqExome62Mb>/usr/local/packages/seq/tools/solexa/TruSeqExome62Mb/modified.targets.txt</TruSeqExome62Mb>
		<ACE3_Personalis>/usr/local/packages/seq/tools/solexa/ACE3_Personalis/modified.targets.txt</ACE3_Personalis>
		<SureSelect75Mbv5UTR>/usr/local/packages/seq/tools/solexa/SureSelect75Mbv5UTR/modified.targets.txt</SureSelect75Mbv5UTR>
		<SureSelect91Mbv6UTR>/usr/local/packages/seq/tools/solexa/SureSelect91Mbv6UTR/modified.targets.txt</SureSelect91Mbv6UTR>
		<SureSelect60Mbv6_200ng_XTLI>/usr/local/packages/seq/tools/solexa/exome60MbV6/modified.targets.txt</SureSelect60Mbv6_200ng_XTLI>
		<SeqCapEZMedExome>/usr/local/packages/seq/tools/solexa/SeqCapEZMedExome/modified.targets.txt</SeqCapEZMedExome>
	</targets>
	<windows>
		<SureSelect38Mb>50</SureSelect38Mb>
		<SureSelect50Mb>50</SureSelect50Mb>
		<SureSelect50Mbv4>50</SureSelect50Mbv4>
		<SureSelect50Mbv5>50</SureSelect50Mbv5>
		<SureSelect60Mbv6>50</SureSelect60Mbv6>
		<genomic>50</genomic>
		<Nextera62Mb>50</Nextera62Mb>
		<TruSightOneV1>50</TruSightOneV1>
		<SeqCapEZExomeV2>50</SeqCapEZExomeV2>
		<SeqCapEZExomeV3>50</SeqCapEZExomeV3>
		<NexteraRapidCaptureV1.2>50</NexteraRapidCaptureV1.2>
		<TruSeqExome62Mb>50</TruSeqExome62Mb>
		<ACE3_Personalis>50</ACE3_Personalis>
		<SureSelect75Mbv5UTR>50</SureSelect75Mbv5UTR>
		<SureSelect91Mbv6UTR>50</SureSelect91Mbv6UTR>
		<SureSelect60Mbv6_200ng_XTLI>50</SureSelect60Mbv6_200ng_XTLI>
		<SeqCapEZMedExome>50</SeqCapEZMedExome>
	</windows>
	<solexadb>
	<host>DBLIMS_HOST</host>
	<port>DBLIMS_PORT</port>
	<database>DBLIMS_DB</database>
	<user>DBLIMS_USER</user>
	<password>DBLIMS_PASSWORD</password>
	</solexadb>
	<dirs>
		<out>out</out>
		<projectfolder>ANALYSISOUT</projectfolder>
		<hg38projectfolder>ANALYSISOUTHG38</hg38projectfolder>
	</dirs>
	<runs>
		<alignmentOnly>
			<run>alignment:4-6</run>
		</alignmentOnly>
		
		<standard>
			<run>bam_merge</run>
			<run>transcriptStats</run>
			<run>stats</run>
			<run>checkContamination</run>
			<run>parsestats</run>
			<run>statsdb</run>
			<run>insertSizeIntoDB</run>
		</standard>
		<exome>
			<run>varfilter</run>
			<run>runPindel</run>
			<run>filterPindel</run>
			<run>annotatePindel</run>
			<run>exomeDepth</run>
			<run>annotateSNPs</run>
			<run>checkPileup</run>
			<run>transversion</run>
			<run>snpEff</run>
			<run>RefSeq</run>
			<run>lncRNA</run>
			<run>miRNA</run>
			<run>callChrM</run>
			<run>annotateChrM</run>
		</exome>
		
		<exomedb>
			<run>insertPindel</run>
			<run>exomeDepthInsert</run>
			<run>snvdbInsertExome</run>
			<run>snpEffInsert</run>
			<run>homozygosity</run>
			<run>updateRefSeq</run>
			<run>updatelncRNA</run>
			<run>updatemiRNA</run>
			<run>insertChrM</run>		
		</exomedb>
		
		<genome>
			<run>deepvariant</run>
			<run>cnvnator</run>
			<run>runBreakdancer</run>
			<run>runLumpy</run>
			<run>runManta:4-6</run>
			<disabled>runBellerophon</disabled>
			<run>runIGVtools</run>
			<run>mergeSV</run>
		</genome>
	
		<genomedb>
			<disabled>insertSV</disabled>
			<disabled>insertBellerophon</disabled>
			<run>genomerun</run>
			<run>genomerun</run>
		</genomedb>
	
		<rnaseq>
			<run>htseqcount</run>
			<run>calcFPKM</run>
			<run>normalization</run>
			<run>RNAdbInsert</run>
			<run>calcRNAstats:4</run>
			<run>RNAStatsDBInsert</run>
		</rnaseq>
		
		<chipseq>
			<run>runMacs</run>
		</chipseq>
		
		<mtDNA>
			<run>runPindel</run>
			<run>filterPindel</run>
			<run>annotatePindel</run>
			<run>insertPindel</run>
			<run>callChrM</run>
			<run>annotateChrM</run>
			<run>insertChrM</run>
		</mtDNA>
		
	</runs>
	<scripts>
		<parseConfig>pipelinecfg_gatk.pl -i</parseConfig>
		<sequence2BAMpl>copyRunFolderFiles.pl</sequence2BAMpl></scripts></config>
